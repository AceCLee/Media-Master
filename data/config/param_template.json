{
    "video_transcoding_cmd_param_template": {
        "x264_veryslow": [
            "{{vspipe_exe_filepath}}",
            "--y4m",
            "{{input_vpy_filepath}}",
            "-",
            "|",
            "{{x264_exe_filepath}}",
            "--demuxer",
            "y4m",
            "-",
            "--output",
            "{{output_video_filepath}}",

            "--preset",
            "veryslow"
        ],
        "x265_slower": [
            "{{vspipe_exe_filepath}}",
            "--y4m",
            "{{input_vpy_filepath}}",
            "-",
            "|",
            "{{x265_exe_filepath}}",
            "--y4m",
            "--output",
            "{{output_video_filepath}}",
            "-",
            "--preset",
            "slower"
        ],
        "nvenc_default": [
            "{{nvenc_exe_filepath}}",
            "--input",
            "{{input_video_filepath}}",
            "--output",
            "{{output_video_filepath}}",
            "--codec",
            "{{codec}}",
            "--output-depth",
            "{{output_depth}}",
            "--cqp",
            "20:23:25"
        ],
        "vspipe_nvenc_default": [
            "{{vspipe_exe_filepath}}",
            "--y4m",
            "{{input_vpy_filepath}}",
            "-",
            "|",
            "{{nvenc_exe_filepath}}",
            "--y4m",
            "--input",
            "-",
            "--output",
            "{{output_video_filepath}}",
            "--codec",
            "{{codec}}",
            "--output-depth",
            "{{output_depth}}",
            "--cqp",
            "20:23:25"
        ]
    },
    "audio_transcoding_cmd_param_template": {
        "flac_test": [
            "{{ffmpeg_exe_filepath}}",
            "-i",
            "{{input_audio_filepath}}",
            "-vn",
            "-sn",
            "-dn",
            "-n",
            "-f",
            "wav",
            "-codec:a",
            "{{ffmpeg_wav_audio_codec}}",
            "-",
            "|",
            "{{flac_exe_filepath}}",
            "-",
            "--best",
            "--ignore-chunk-sizes",
            "--force",
            "-o",
            "{{output_filepath}}"
        ],
        "qaac_test": [
            "{{ffmpeg_exe_filepath}}",
            "-i",
            "{{input_audio_filepath}}",
            "-vn",
            "-sn",
            "-dn",
            "-n",
            "-f",
            "wav",
            "-codec:a",
            "{{ffmpeg_wav_audio_codec}}",
            "-",
            "|",
            "{{qaac_exe_filepath}}",
            "--ignorelength",
            "-o",
            "{{output_filepath}}",
            "-"
        ],
        "opus_128": [
            "{{opusenc_exe_filepath}}",
            "--bitrate",
            "128.0",
            "--vbr",
            "{{input_audio_filepath}}",
            "{{output_filepath}}"
        ]
    }
}